{
  "version": 2,
  "functions": {
    "api/index.go": {
      "runtime": "go1.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.go"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "installCommand": "cd frontend && npm install",
  "buildCommand": "cd frontend && npm run vercel-build",
  "outputDirectory": "frontend/public",
  "env": {
    "NODE_ENV": "production"
  }
}